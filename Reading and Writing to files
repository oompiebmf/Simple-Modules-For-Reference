open_file = open("toot", 'w')
for count in range(0,100):
    open_file.write(str(count) + "\n")
open_file.close()

open_file_lines = open("toot", 'r').readlines()
for line in open_file_lines:
    print line

open_file = open("toot", 'a')
for letters in range(0,100):
    open_file.write(chr(letters) + "\n")
    
open_file.close()

open_file_lines = open("toot", 'r').readlines()
for line in open_file_lines:
    print line
